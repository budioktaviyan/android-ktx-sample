<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="id.kotlin.sample.ktx">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name="id.kotlin.sample.ktx.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="id.kotlin.sample.ktx.sharedprefs.SharedPrefsActivity" />
        <activity android:name="id.kotlin.sample.ktx.uri.UriActivity" />
        <activity android:name="id.kotlin.sample.ktx.htmlencode.HtmlEncodeActivity" />
    </application>
</manifest>